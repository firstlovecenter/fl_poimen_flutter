{
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^16.2.4",
    "generate-changelog": "^1.8.0",
    "husky": "^8.0.1"
  },
  "scripts": {
    "start": "flutter run",
    "api": "cd ../fl-admin-portal/api &&  npm run start:dev",
    "release:major": "node scripts/release.js major",
